generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model db_user {
  id       String @id(map: "user_pk") @db.VarChar(255)
  password String @db.VarChar(255)
}

model test {
  id      Int       @id(map: "test_pk") @default(autoincrement())
  content String    @db.NVarChar(255)
  clock   DateTime?
  month   Int?
}
